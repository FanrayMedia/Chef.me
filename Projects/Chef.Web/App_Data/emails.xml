<?xml version="1.0" encoding="UTF-8" ?>
<emails>
<!--
======================================================
    Email Me
======================================================
-->
  <email emailType="EmailMe">
    <subject>{s.firstName} {s.lastName} sent you a message on Chef.me</subject>
    <htmlbody>
<![CDATA[      
      <table cellspacing="0" cellpadding="0" style="padding:0;margin:0px;background:#ffffff;display:block;width:720px;font-family:Helvetica,Sans-Serif">
        <tbody style="display:block;width:720px">
          <tr style="display:block;width:720px">
            <td style="background:#00679a;width:720px;display:block;height:10px"></td>
          </tr>
          <tr style="display:block;width:720px">
            <td style="padding:18px 60px 16px 56px">
              <table style="width:604px;font-family:Helvetica,Sans-Serif">
                <tbody>
                  <tr>
                    <td align="left" style="width:50%">
                      <a href="http://www.chef.me" target="_blank" style="text-decoration:none;color:#2b82ad">
                        <h1>Chef.me</h1>
                      </a>
                    </td>
                    <td align="right" style="width:50%;padding:0px 0px 0px 0px">
                      <p style="color:#6a6f73;font-size:14px;margin:27px 0 0 0">
                        Hi <b>{r.firstName}</b>!<br/>
                          <a style="color:#2b82ad;text-decoration:none;display:inline-block;margin:5px 0 0 0"
                          href="http://www.chef.me/{r.userName}" target="_blank">View Your Profile</a>
                      </p>
                    </td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
          <tr style="display:block;width:604px">
            <td style="padding:0px 59px 20px 56px;display:block;width:604px;height:1px">
              <table style="display:block;width:604px;font-family:Helvetica,Sans-Serif;height:1px">
                <tbody>
                  <tr style="display:block;height:1px">
                    <td align="left" style="display:block;width:604px;background:#e8ebed;height:1px;padding:0;margin:0"></td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
          <tr style="display:block;width:100%">
            <td style="padding:1px 75px 10px 56px;display:block">
              <table style="width:100%;font-family:Helvetica,Sans-Serif">
                <tbody>
                  <tr>
                    <td align="left" style="width:100%">
                      <p style="padding:0;margin:0;color:#322f31;font-size:26px;font-weight:500">
                        <a href="http://www.chef.me/" style="color:#2b82ad;text-decoration:none" target="_blank">{s.firstName} {s.lastName}</a> sent you a message
                      </p>
                    </td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
          <tr style="display:block;width:100%">
            <td style="padding:1px 75px 50px 56px;display:block">
              <table style="width:100%;font-family:Helvetica,Sans-Serif">
                <tbody>
                  <tr>
                    <td align="left" valign="top" style="width:218px;margin:0;padding:10px 0 0 0">
                      <a href="http://www.chef.me/{s.userName}" style="text-decoration:none" target="_blank">
                        <img height="128" width="128" src="{s.bgImgUrl}" style="border:1px solid #393939; padding:2px"/>
                      </a>
                    </td>
                    <td align="left" valign="top" width="68%">
                      <p style="font-size:15px;padding:6px 0 0 0px;margin:0 0 0 0;line-height:21px">
                        {s.message}
                      </p>
                      <br/>
                        <a href="mailto:{s.email}" style="color:#ffffff" target="_blank">
                          <span style="white-space:nowrap;text-transform:uppercase;font-weight:bold;font-size:14px;color:#ffffff;padding:0 30px;line-height:33px;display:inline-block;border:1px #4e9903 solid;background:rgb(94,166,53);background:-moz-linear-gradient(top,rgba(94,166,53,1) 0%,rgba(94,166,53,1) 50%,rgba(78,153,3,1) 51%,rgba(78,153,3,1) 100%);background:-webkit-linear-gradient(top,rgba(94,166,53,1) 0%,rgba(94,166,53,1) 50%,rgba(78,153,3,1) 51%,rgba(78,153,3,1) 100%);background:-o-linear-gradient(top,rgba(94,166,53,1) 0%,rgba(94,166,53,1) 50%,rgba(78,153,3,1) 51%,rgba(78,153,3,1) 100%);background:-ms-linear-gradient(top,rgba(94,166,53,1) 0%,rgba(94,166,53,1) 50%,rgba(78,153,3,1) 51%,rgba(78,153,3,1) 100%);background:linear-gradient(to bottom,rgba(94,166,53,1) 0%,rgba(94,166,53,1) 50%,rgba(78,153,3,1) 51%,rgba(78,153,3,1) 100%);border-radius:5px">
                            Reply to {s.firstName}
                          </span>
                        </a>
                    </td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
          <tr style="display:block;width:100%">
            <td style="padding:1px 75px 10px 56px;display:block">
              <table style="width:100%;font-family:Helvetica,Sans-Serif">
                <tbody>
                  <tr>
                    <td align="left" style="width:100%">
                      <p style="padding:0;margin:0;color:#81878c;font-size:13px;font-weight:500">
                        This email was sent to you by <a style="color:#2b82ad;text-decoration:none"
                href="http://www.chef.me/{s.userName}" target="_blank">chef.me/{s.userName}</a>,
                        and is not an official communication from <span>chef.me</span>.
                      </p>
                    </td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
          <tr style="display:block;width:100%">
            <td style="padding:0px 75px 50px 56px;display:block">
              <table style="width:100%;font-family:Helvetica,Sans-Serif">
                <tbody>
                  <tr>
                    <td align="left" style="width:100%">
                      <p style="color:#322f31;font-size:16px;margin:0">Thank you,</p>
                      <p style="color:#2b82ad;text-decoration:none;font-size:16px;margin:5px 0 30px 0">
                        The <a style="color:#2b82ad;text-decoration:none" href="http://www.chef.me" target="_blank">chef.me</a> team
                      </p>
                    </td>
                  </tr>
                  <tr>
                    <td align="left" style="color:#9daab3;font-size:11px">
                      To no longer receive these emails, go to 
                      <a style="color:#9daab3;font-size:11px;text-decoration:underline" href="http://www.chef.me/{r.userName}" target="_blank">settings/privacy</a>
                      to turn it off.
                    </td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
        </tbody>
      </table>
   ]]>   
    </htmlbody>
  </email>

</emails>